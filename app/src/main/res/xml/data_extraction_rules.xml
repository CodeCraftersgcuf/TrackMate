<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- Allow extraction of shared preferences -->
    <cloud-backup include="true">
        <include domain="sharedpref" />
    </cloud-backup>

    <!-- Block extraction of files in the cache directory -->
    <device-transfer include="true">
        <exclude domain="file" path="cache/" />
    </device-transfer>
</data-extraction-rules>
